<%- include('../includes/head.ejs') %>
<link rel="stylesheet" href="/css/model.css">
</head>

    <body>
        <%- include('../includes/navigation.ejs') %>
        <main class="centered">
            <h1><%= model.principal_name %></h1>
			<% if (model.aliases != null) { %>
                <p>Also known as: <%= model.aliases %></p>
            <% } else { %>
                <p>No Aliases<p>
            <% } %>
            <hr>
            <div class="parent">
                <div class="child">
                    <div class="grid">
                        <article class="card model-item-2">
                            <div class="model__detail__image">
                                <img src="<%= model.FA_url %>" alt="<%= model.principal_name %>">
                            </div>
                        </article>
                        <article class="card model-item-2">
                            <div class="model__detail__image">
                                <img src="<%= model.BR_url %>" alt="<%= model.principal_name %>">
                            </div>
                        </article>
                        <article class="card model-item-2">
                            <div class="model__detail__image">
                                <img src="<%= model.PF_url %>" alt="<%= model.principal_name %>">
                            </div>
                        </article>
                        <article class="card model-item-2">
                            <div class="model__detail__image">
                                <img src="<%= model.PR_url %>" alt="<%= model.principal_name %>">
                            </div>
                        </article>
                        <article class="card model-item-2"  >
                            <div class="model__detail__image">
                                <img src="<%= model.AR_url %>" alt="<%= model.principal_name %>">
                            </div>
                        </article>
                    </div>
                </div>
                <div class="child">
                    <img src="<%= model.RF_url %>" alt="<%= model.thumbnail %>">
                </div>
            <h2><%= model.comment %></h2>
   
        </main>
        <%- include('../includes/end.ejs') %>