<%- include('../includes/head.ejs') %>
<link rel="stylesheet" href="/css/main.css">
<link rel="stylesheet" href="/css/forms.css">
</head>
    <body>
        <%- include('../includes/navigation.ejs') %>
		<main>
			<div class="container">
				<h1 class="text-center text-primary mt-3 mb-3">Start of model addition</h1>

				<h2 class="card__header">Attributes</h2>
				<div class="Row">
					<div class="card card__content">
						<p>ASHP</p>
						<select name="ashp" id="ashp" class="form-control select">
							<option value="">Arse Shape</option>
							<% for(var count = 0; count < ashp_list.length; count++) { %>
								<option value="<%= ashp_list[count].l2_desc %>"><%= ashp_list[count].l2_desc %></option>
							<% } %>
						</select>
					</div>
					<div class="Column">
						<p class="card__content">NATN</p>
						<select name="natn" id="natn" class="form-control select">
							<option value="">Nationality</option>
							<% for(var count = 0; count < natn_list.length; count++) { %>
								<option value="<%= natn_list[count].l2_desc %>"><%= natn_list[count].l2_desc %></option>
							<% } %>
						</select>
					</div>
					<div class="Column">
						<p class="card__content">EYES</p>
						<select name="eyes" id="eyes" class="form-control select">
							<option value="">Eyes</option>
							<% for(var count = 0; count < eyes_list.length; count++) { %>
								<option value="<%= eyes_list[count].l2_desc %>"><%= eyes_list[count].l2_desc %></option>
							<% } %>
						</select>
					</div>
					<div class="Column">
						<p class="card__content">HAIR</p>
						<select name="hair" id="hair" class="form-control select">
							<option value="">Hair</option>
							<% for(var count = 0; count < hair_list.length; count++) { %>
								<option value="<%= hair_list[count].l2_desc %>"><%= hair_list[count].l2_desc %></option>
							<% } %>
						</select>
							</div>
				</div>
			</div>
		</main>
            <%- include('../includes/end.ejs') %>